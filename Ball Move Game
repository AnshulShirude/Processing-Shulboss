int x = 200;
int y = 200;

void setup()
{
  size (400,400);
  smooth();
  background(0);
  noStroke();
  fill(0,255,0);
}

void draw()
{
  ellipse(x, y, 40, 40);
  
  if (key == 'd'){
    x = x + 1;
  }
  
    if (key == 'a'){
    x = x - 1;
    }
    
    if (key == 's'){
    y = y + 1;
    }
    
    if (key == 'w'){
    y = y - 1;
    }
    
    if (key == 'q'){
      x=0;
      y = 0;
    }
}
